{
  "manifest_version": 3,
  "name": "Scraper Extension",
  "version": "1.0",
  "description": "Scrapes elements from the current tab.",
  "permissions": ["tabs", "activeTab", "scripting"],
  "host_permissions": [
    "https://alpha.mycourseville.com/",
    "https://mycourseville.com/"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
