{
  "manifest_version": 3,
  "name": "Scraper Extension",
  "version": "1.0",
  "description": "Scrapes elements from the current tab.",
  "permissions": ["tabs", "activeTab", "scripting", "<all_urls>"],
  "background": {
    "service_worker": "build/background.js"
  },
  "action": {
    "default_popup": "build/popup.html"
  }
}
