{
  "manifest_version": 3,
  "name": "ourcourseville",
  "version": "1.0",
  "description": "Productivity tool for students",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": ["tabs", "activeTab", "scripting"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "api_url": "http://localhost:5203/api/v1",
  "api_key": "apikey"
}
